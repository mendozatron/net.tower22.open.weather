  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/mendozatron/net.tower22.open.weather.git"
  }
}
